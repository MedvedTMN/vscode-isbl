{
	"Region Start": {
		"prefix": "//#region",
		"body": [
			"//#region $0"
		],
		"description": "Folding Region Start"
	},
	"Region End": {
		"prefix": "//#endregion",
		"body": [
			"//#endregion"
		],
		"description": "Folding Region End"
	},
	"If Statement": {
		"prefix": "if",
		"body": [
			"if ${1:condition}",
			"\t$0",
			"endif"
		],
		"description": "If Statement"
	},
	"If-Else Statement": {
		"prefix": "ifelse",
		"body": [
			"if ${1:condition}",
			"\t$0",
			"else",
			"\t",
			"endif"
		],
		"description": "If-Else Statement"
	},
	"IfThen Statement": {
		"prefix": "ifthen",
		"body": [
			"IfThen(${1:condition}; ${2:true}; ${3:false})"
		],
		"description": "IfThen Statement"
	},
	"For-Each Loop": {
		"prefix": "foreach",
		"body": [
			"foreach ${1:element} in ${2:array}",
			"\t$0",
			"endforeach"
		],
		"description": "For-Each Loop"
	},
	"While Statement": {
		"prefix": "while",
		"body": [
			"while ${1:condition}",
			"\t$0",
			"endwhile"
		],
		"description": "While Statement"
	},
	"Try-Except Statement": {
		"prefix": "tryexcept",
		"body": [
			"try",
			"\t$0",
			"except",
			"\t",
			"endexcept"
		],
		"description": "Try-Except Statement"
	},
	"Try-Finally Statement": {
		"prefix": "tryfinally",
		"body": [
			"try",
			"\t$0",
			"finally",
			"\t",
			"endfinally"
		],
    "description": "Try-Finally Statement"
  }
}